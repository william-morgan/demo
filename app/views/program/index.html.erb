   <div class="programs">
    <ul  class="program-list">
    	<% @programs.each do |program| %>
        <li class="card text-xs-center">
		    <div class="card-header"><%= program.name %></div>
		    <div class="card-block"><%= raw CGI.unescapeHTML program.description %></div>
		    <div class="card-footer">
		    	<a class="btn btn-secondary btn-launch" data-title="<%= program.name_shorthand %> Application"
		    		                                    data-id="<%= program.id %>" 
		    		                                    data-target="#modal" 
		    		                                    data-toggle="modal"  
		    		                                    ui-sref="program-launch({id:'<%= program.id %>'})"><%= program.launch_text %></a>   
		    </div>       	
        </li>
    	<% end %>
    </ul>
    </div>
